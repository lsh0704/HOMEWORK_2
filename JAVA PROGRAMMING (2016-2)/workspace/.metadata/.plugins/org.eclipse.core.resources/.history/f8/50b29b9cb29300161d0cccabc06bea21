import java.util.Scanner;

public class StudentTest {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		Human man[] = new Human[3]; //휴먼 크기
		Student stu[] = new Student[3]; //학생 크기
		int i;
		String name, major; int age, sID;
		
		for (i=1; i<=3; i++){ //사용자로부터 이름과 나이를 입력 받아서 3명의 Human 객체를 생성
			System.out.print("["+i+"] Human 입력: ");
			name = scan.next();
			age = scan.nextInt();
			man[i-1]=new Human(name, age); //객체 생성
			
			if (i>=2)
				man[i].equals(man[i-1]);
		}		
		for (i=1; i<=3; i++){ //사용자로부터 이름,나이,전공,학번을 입력 받아서 3명의 Student 객체 생성
			System.out.print("["+i+"] Student 입력: ");
			name = scan.next();
			age = scan.nextInt();
			major = scan.next();
			sID = scan.nextInt();
			
			stu[i-1]=new Student(name, age, major, sID); //객체 생성
		}
		
		for (i=0; i<3; i++){ //각 객체참조변수 이름으로 출력문 작성
			System.out.println(man[i].toString());
		}
		for (i=0; i<3; i++){ //각 객체참조변수 이름으로 출력문 작성
			System.out.println(stu[i].toString());
		}
	}
}